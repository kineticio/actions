{
  "name": "@runnablejs/app",
  "version": "0.3.13",
  "sideEffects": false,
  "main": "build/index.js",
  "types": "app/types.ts",
  "files": [
    "app/types.ts",
    "app/api",
    "build",
    "public"
  ],
  "scripts": {
    "build": "remix build",
    "dev": "ACTIONS_BASE_URL='/admin' remix dev",
    "prepublishOnly": "pnpm build",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "jest --config jest.config.js",
    "typecheck": "tsc --noEmit",
    "types": "tsc -p tsconfig.types.json",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/server-runtime": "^1.7.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@chakra-ui/icons": "^2.0.12",
    "@chakra-ui/react": "^2.4.1",
    "@chakra-ui/styled-system": "^2.3.5",
    "@chakra-ui/system": "^2.3.3",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@iconify-json/fa": "^1.1.2",
    "@iconify/react": "^4.0.0",
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@runnablejs/config": "*",
    "@types/jest": "29.2.3",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "binode": "^1.0.5",
    "chakra-multiselect": "^0.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "framer-motion": "^7.6.7",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "react-remove-scroll": "^2.5.5",
    "remix-mount-routes": "^0.1.3",
    "stylis": "^4.1.3",
    "tiny-invariant": "^1.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0"
  }
