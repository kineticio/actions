{
  "name": "@runnablejs/app",
  "version": "0.4.15",
  "sideEffects": false,
  "main": "build/index.js",
  "types": "app/types.ts",
  "files": [
    "app/types.ts",
    "app/api",
    "build",
    "public"
  ],
  "scripts": {
    "build": "remix build",
    "watch": "remix watch",
    "dev": "run-p dev:server watch",
    "dev:server": "tsx server/server.ts",
    "build:server": "esbuild --platform=node --format=cjs ./server/server.ts --outdir=build --bundle",
    "start:server": "cross-env RUNNABLE_BASE_URL='/' NODE_ENV=production node ./build/server.js",
    "prepublishOnly": "pnpm build",
    "format": "prettier --write .",
    "lint": "TIMING=1 eslint . --fix",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "types": "tsc -p tsconfig.types.json",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.6",
    "@chakra-ui/styled-system": "^2.12.0",
    "@chakra-ui/system": "^2.6.2",
    "@chakra-ui/theme-tools": "^2.2.6",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@iconify-json/fa": "^1.2.1",
    "@iconify/react": "^4.1.1",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@remix-run/express": "^1.19.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/server-runtime": "^1.19.3",
    "@runnablejs/api": "*",
    "@runnablejs/config": "*",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-prometheus-middleware": "^1.2.3",
    "@types/jest": "29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.set": "^4.3.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.19.79",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^9.0.8",
    "binode": "^1.0.5",
    "chakra-multiselect": "^0.4.13",
    "compression": "^1.8.0",
    "cross-env": "^7.0.3",
    "esbuild": "0.25.0",
    "eslint": "^8.57.1",
    "express": "^4.21.2",
    "express-prometheus-middleware": "^1.2.0",
    "framer-motion": "^10.18.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "prom-client": "^14.2.0",
    "react": "^18.3.1",
